<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evmobile.eversvik.stationmonitor">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:icon="@drawable/intercomicon"
        >

        <receiver android:name=".src.activity.ServiceAutoStart">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapService"
            android:enabled="true"
            android:exported="true"
            />

        <activity
            android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDeviceCallActivity"
            android:label="Intercom Calls" />
        <activity
            android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice1Activity"
            android:label="@string/device_activity_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice2MenuActivity"
            android:parentActivityName="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice1Activity"/>

        <activity android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDeviceCreateActivity"
            android:parentActivityName="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice1Activity"/>

        <activity android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDeviceCallNewActivity"
            android:parentActivityName="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice2MenuActivity" />

        <activity android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDeviceGpoActivity"
            android:parentActivityName="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice2MenuActivity" />

        <activity android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDeviceGpiActivity"
            android:parentActivityName="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice2MenuActivity"/>

        <activity android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDeviceActionActivity"
            android:parentActivityName="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice2MenuActivity"/>

        <activity android:name="com.evmobile.eversvik.stationmonitor.src.activity.ZapDeviceNotifyActivity"
            android:parentActivityName="com.evmobile.eversvik.stationmonitor.src.activity.ZapDevice2MenuActivity"/>
    </application>

</manifest>
